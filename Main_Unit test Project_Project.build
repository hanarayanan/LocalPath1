            <arg value="${filename}" />
            <arg value="/property:Configuration=${build.configuration}" />
            <arg value="/flp:logfile=${build.logFilePath}/${build.logFileName}" />
          </exec>
        </if>
        <if test="${string::ends-with(var1, 'csproj') }">
